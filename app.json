{
  "expo": {
    "name": "Ayuuto",
    "slug": "ayuuto",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ayuuto",
    "associatedDomains": ["applinks:yourdomain.com"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ayuuto.app",
      "buildNumber": "3",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized experiences.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to share images.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos.",
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "104.248.117.205": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.png",
        "backgroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ayuuto.app",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a2332",
          "dark": {
            "backgroundColor": "#1a2332"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#1a2332",
          "sounds": []
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#1a2332",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "986fa7c8-65c5-4a79-bda3-7149c2f8f017"
      }
    },
    "owner": "technoarts104"
  }
}
